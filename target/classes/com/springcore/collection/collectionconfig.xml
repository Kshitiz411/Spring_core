<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xmlns:context="http://www.springframework.org/schema/context" 
 xmlns:p="http://www.springframework.org/schema/p"
 xsi:schemaLocation=" http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
 http://www.springframework.org/schema/context 
 http://www.springframework.org/schema/context/spring-context-3.0.xsd"> 
<!-- this is our beans  -->
<bean class="com.springcore.collection.Emp" name="emp1">

<property name="name" value="Kshitiz"/>
 
 <property name="phones">
  
  <list>
    <value>9860931242</value>
    <value>9811131467</value>
    <value>9814213812</value>
  </list>
 </property>
 
 <property name="address">
  <set>
    <value>Kathmandu</value>
    <value>Pokhara</value>
    <value>Nepalgunj</value>
  </set>
 </property>
 
 <property name="courses">
  <map>
   <entry key="java" value="2 months"/>
   <entry key="python" value="1.5 months"/>
   <entry key="hibernate" value="1.5 months"/>
  </map>
 
</property>
</bean>


</beans>
